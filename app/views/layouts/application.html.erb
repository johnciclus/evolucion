                    <li id="vaexo-fyn" class="ui-state-default ui-corner-all" title="Variable exogena"><span></span></li>
                    <li id="retar-fyn" class="ui-state-default ui-corner-all" title="Retardo"><span></span></li>
                    <li id="multi-fyn" class="ui-state-default ui-corner-all" title="Multiplicador"><span></span></li>
